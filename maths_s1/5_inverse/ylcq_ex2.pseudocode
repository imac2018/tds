det(A) {
    assert(est_carree(A))
    pour ligne de A
        si ligne est nulle
            renvoyer 0
    A = triangulariser(A)
    prod = 1;
    pour i dans [0, A.n_lignes]
        pour j dans [0, A.n_colonnes]
            prod *= A(i,j)
    renvoyer prod
}

